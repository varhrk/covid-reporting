										"method": "POST",
										"headers": {},
										"url": "https://fdfd",
										"body": "dummy"
									}
								},
								{
									"name": "Fail of population source",
									"type": "Fail",
									"dependsOn": [],
									"userProperties": [],
									"typeProperties": {
										"message": "file is not correct",
										"errorCode": "404"
									}
								}
							],
							"ifTrueActivities": [
								{
									"name": "Copy population",
									"type": "Copy",
									"dependsOn": [],
